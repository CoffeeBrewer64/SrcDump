bank0	group	G_BANK0

	public	init_bitplay
	public	bitplay


;-------------------------------------------------------;
;							;
;		 Bit Sound Play Routine			;
;							;
; Play Data Make Usage					;
; (1)Amiga   : MIC Cable To Printer-Port-SamplingBox In	;
; (2)        : Audio Media II Sampling			;
;            : Sampling Rate 30858			;
; (3)Dos/V   : bin2txtc(BIN->TXT)			;
; (4) mi     : File Edit (0xA0,0xCC -> A0 CC)		;
; (5) convert: \home\jun\bitcnv\bitcnv file > data.dat	;
;            : Convert Option : -i -d (Use?)		;
; (6) cp     : cp data.dat ..\pink\sound		;
; (7)Unix    : touch -c bitplay.dmg			;
; (8)Dos/V   : make					;
;-------------------------------------------------------;
bitplay:
	ret
 ifn 0
	ld	a,(mons_bitplay_adrs+0)	; Bit Play Monster Voice Address
	ld	l,a
	ld	a,(mons_bitplay_adrs+1)	; Bit Play Monster Voice Address
	ld	h,a
	ld	a,(hli)
	ld	c,a			; Get Data Size Low
	ld	a,(hli)
	ld	b,a			; Get Data Size High
loop11$:
	ld	e,$08			;2	Cycle Check
	ld	a,(hli)			;2
	ld	d,a			;1
	dec	bc			;2
	ld	a,b			;1
	or	b			;1
	jr	z,end$			;==3,!=2
	jr	loop5$			;3 (14 Cycle)
loop1$:
;	nop				;1
;	nop				;1
loop2$:
;	nop				;1
;	nop
;	nop
;	nop
;	nop
;	nop
;	nop
;	nop
;	nop
;	nop
;	nop
;	nop				; 14 Cycle
loop5$:
	push	bc			; Wait
;	nop				; Wait
;	nop				; Wait
;	nop				; Wait
	pop	bc			; Wait

	xor	a			;1
	sla	d			;2
	rra				;1
	rra				;1
	rra				;1
	ld	(NR32),a		;2
	dec	e			;1
	jr	nz,loop2$		;==3,!=2
	jr	loop11$			;3
end$:
	ld	a,BITPLAY_BANK_NO		; After Function Bank Number
	ld	(rombankno),a
	ld      (ROMBANK),a
	jp	after_bitplay
 endif


bank20	group	G_BANK20

	org	4000h


;-------------------------------------------------------;
;							;
;		Initialize Of Bit Play			;
;							;
; OUT	Areg : Bit Data Bank Number			;
;-------------------------------------------------------;
init_bitplay:
	ret
 ifn 0
	ld	a,(mons_bitplay_wait)	; Bit Play Voice Start Wait Counter
	inc	a
	cp	4			; Voice Start Wait Count
	jr	z,start$

	ld	(mons_bitplay_wait),a	; Bit Play Voice Start Wait Counter

;---------------------------------------; Change By Tetsu 98/02/13
	extern	wait_vb
	call	wait_vb
	jr	init_bitplay
;	ret				; Non ZERO
;---------------------------------------;


start$:
;---------------------------------------; Fook By Tetsu 98/02/09
 ifn 0					
	ld	a,(ev_t1_3)
	set	5,a
	ld	(ev_t1_3),a

	ld	hl,zukan_flg2		; DEBUG!!
	ld	b,19

l$:
	ld	a,0ffh			; DEBUG!! ZUKAN FULL!
	ld	(hli),a
	dec	b
	jr	nz,l$
 endif
;---------------------------------------;


	ld	a,080h			; All Sound On (Sound 3 Off)
	ld	(NR52),a
	ld	a,077h			; Volume Max
	ld	(NR50),a

	xor	a
	ld	(NR30),a
	ld	hl,$ff30		; Wave RAM Area
	ld	de,haji_buf		; Wave Backup
loop55$:
	ld	a,(hl)			; Get Now Wave
	ld	(de),a
	inc	de
	ld	a,$ff			; Wave Data
	ld	(hli),a			; Wave Set
	ld	a,l
	cp	$40			; ff30-ff3f
	jr	nz,loop55$

	ld	a,$80
	ld	(NR30),a		; Output On

	ld	a,(NR51)
	or	$44			; Sound 3 Only
	ld	(NR51),a		; Wave Only On
	ld	a,$80
	ld	(NR34),a		; Init & Count

	ld	a,(mons_bitplay_num)	; Monster Voice Number
	dec	a
	ld	e,a
	ld	l,a
	xor	a
	ld	d,a
	ld	h,a
	add	hl,hl			; * 2
	add	hl,de			; * 3
	ld	de,bit_tbl	
	add	hl,de

	ld	a,(hli)			; Get Data Address Low
	ld	(mons_bitplay_adrs+0),a	; Bit Play Monster Voice Address
	ld	a,(hli)			; Get Data Address High
	ld	(mons_bitplay_adrs+1),a	; Bit Play Monster Voice Address
	ld	a,(hl)			; Get Bank Number
	cp	(hl)			; ZERO Flag ON
	ret

;-------------------------------------------------------;
;							;
;	      After Function Of Bit Play		;
;							;
;-------------------------------------------------------;
after_bitplay:
	xor	a
	ld	(mons_bitplay_num),a	; Monster Voice Number Clear
	ld	(mons_bitplay_wait),a	; Voice Start Wait Clear

	ld	a,080h			; All Sound On (Sound 3 Off)
	ld	(NR52),a

	xor	a
	ld	(NR30),a
	ld	hl,$ff30		; Wave RAM Area
	ld	de,haji_buf		; Wave Backup Area
loop56$:
	ld	a,(de)			; Get Backup Wave
	inc	de
	ld	(hli),a			; Wave Set
	ld	a,l
	cp	$40			; ff30-ff3f
	jr	nz,loop56$

	ld	a,$80
	ld	(NR30),a		; Output On

	ld	a,(NR51)
	and	$bb			; Sound 3 Off
	ld	(NR51),a

;---------------------------------------; Add By Tetsu 98/02/09
	xor	a			; SEmode Clear (for se_wait)
	ld	(condetion+4),a
	ld	(condetion+5),a
	ld	(condetion+6),a
	ld	(condetion+7),a
;---------------------------------------;
	ret





