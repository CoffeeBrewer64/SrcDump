 ifn 0
;=======================================================;
;							;
;	PIKA VOICE PLAY					;
;							;
;=======================================================;
	public	PikaVoicePlay
PikaVoicePlay:
	ld	a,e
	and	00000011b
	ld	e,a
	ld	d,00
	ld	hl,VoiceTable
	add	hl,de
	add	hl,de
	ld	a,(hli)
	ld	h,(hl)
	ld	l,a

	ld	c,4
init_loop$:
	dec	c
	jr	z,start$

	call	wait_vb
	jr	init_loop$

start$:
	di

	push	hl
	ld	a,080h			; All Sound On (Sound 3 Off)
	ld	(NR52),a
	ld	a,077h			; Volume Max
	ld	(NR50),a

	xor	a
	ld	(NR30),a
	ld	hl,$ff30		; Wave RAM Area
	ld	de,haji_buf		; Wave Backup
backup_loop$:
	ld	a,(hl)			; Get Now Wave
	ld	(de),a
	inc	de
	ld	a,0ffh			; Wave Data
	ld	(hli),a			; Wave Set
	ld	a,l
	cp	040h			; ff30-ff3f
	jr	nz,backup_loop$

	ld	a,080h
	ld	(NR30),a		; Output On

	ld	a,(NR51)
	or	044h			; Sound 3 Only
	ld	(NR51),a		; Wave Only On

	ld	a,0ffh
	ld	(NR31),a		; Init & Count
	ld	a,00100000b
	ld	(NR32),a		; Init & Count
	ld	a,0ffh
	ld	(NR33),a		; Init & Count
	ld	a,87h
	ld	(NR34),a		; Init & Count

	pop	hl
	call	PikaBitPlayMain

	xor	a
	ld	(mons_bitplay_num),a	; Monster Voice Number Clear
	ld	(mons_bitplay_wait),a	; Voice Start Wait Clear

	ld	a,080h			; All Sound On (Sound 3 Off)
	ld	(NR52),a

	xor	a
	ld	(NR30),a
	ld	hl,0ff30h		; Wave RAM Area
	ld	de,haji_buf		; Wave Backup Area
recover_loop$:
	ld	a,(de)			; Get Backup Wave
	inc	de
	ld	(hli),a			; Wave Set
	ld	a,l
	cp	040h			; ff30-ff3f
	jr	nz,recover_loop$

	ld	a,080h
	ld	(NR30),a		; Output On

	ld	a,(NR51)
	and	0bbh			; Sound 3 Off
	ld	(NR51),a

	xor	a			; SEmode Clear (for se_wait)
	ld	(condetion+4),a
	ld	(condetion+5),a
	ld	(condetion+6),a
	ld	(condetion+7),a
	ei
	ret

VoiceTable:
	dw	bpmPIKA3
	dw	bpmPIKA4
	dw	bpmPIKA2
	dw	bpmPIKA1

;-------------------------------------------------------;
;	Main						;
;-------------------------------------------------------;
PikaBitPlayMain:
	ld	a,(hli)
	ld	c,a
	ld	a,(hli)
	ld	b,a
main_loop0$:
	ld	a,(hli)			;2
	ld	d,a			;1

	ld	a,3			;2
wait_loop$:
	dec	a
	jr	nz,wait_loop$		;16+2

	call	DataGet$
	call	wait$			;30			

	call	DataGet$
	call	wait$			;30			

	call	DataGet$
	call	wait$			;30			

	call	DataGet$
	call	wait$			;30			

	call	DataGet$
	call	wait$			;30			

	call	DataGet$
	call	wait$			;30			

	call	DataGet$
	call	wait$			;30			

	call	DataGet$

	dec	bc			;2
	ld	a,c			;1
	or	b			;1
	jr	nz,main_loop0$		;3/2
	ret

DataGet$:
	ld	a,d			;1
	and	10000000b		;2
	srl	a			;2
	srl	a			;2
	ld	(NR32),a		;2
	sla	d			;2
	ret
wait$:
	ld	a,3			;2
wait_loop1$:
	dec	a
	jr	nz,wait_loop1$		;16+2
	ret			


