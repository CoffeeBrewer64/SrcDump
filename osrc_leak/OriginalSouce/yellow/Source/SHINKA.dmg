banke	group	14

shinka_0:
	ld	hl,ctrl_move_val + SHINKA_MAKE_FLG
	xor	a
	ld	(hl),a
	ld	a,(sel_item_pos)
	ld	c,a
	ld	b,1			; bit set
	call	call_b_ctrl

;************************************************
; shinka_chk					*
;************************************************
shinka_chk:
	ld	a,(wave_flg)
	push	af
	xor	a
	ld	(shinka_flg),a
;	ld	a,0ffh
	dec	a
	ld	(sel_item_pos),a
	push	hl
	push	bc
	push	de
	ld	hl,my_cap_tbl

shinka_1:					; in data : sel_item_pos
	push	hl				; my_cap_tbl save

loop1$:
	ld	hl,sel_item_pos
	inc	(hl)

	pop	hl				; my_cap_tbl load
	inc	hl
	ld	a,(hl)				; monster number
	cp	0ffh
	jp	z,ret$

	ld	(yes_no_map),a			;monster_no. save
	
	push	hl				; my_cap_tbl save

	ld	a,(sel_item_pos)
	ld	c,a
	ld	hl,ctrl_move_val + SHINKA_MAKE_FLG
	ld	b,2				; bit check
	call	call_b_ctrl
	ld	a,c
	and	a
	jp	z,loop1$
	
	ld	a,(yes_no_map)			; monster No
	dec	a
	ld	b,0
	ld	hl,shinka_tbl
	add	a,a
	rl	b
	ld	c,a
	add	hl,bc
	ld	a,(hli)
	ld	h,(hl)
	ld	l,a

	push	hl				; shinka data tbl address save
	ld	a,(sel_item_no)
	push	af

	xor	a
	ld	(my_or_gein),a
	call	set_monsdata_dmy		; mons_tbl -> mons_data
						; my_cap_data -> monsdata_dmy
	pop	af
	ld	(sel_item_no),a
	pop	hl				; shinka data tbl address load
loop2$:
	ld	a,(hli)			; shinka data
	and	a
	jr	z,loop1$

	ld	b,a
	cp	3			; tuushin shinka
	jr	z,tuushin_shinka$

	ld	a,(tuushin_flg)
	cp	50
	jr	z,loop1$		; tuushin koukan chu

	ld	a,b
	cp	2			; item shinka
	jr	z,item_shinka$

	ld	a,(ctrl_move_val + CANCEL_OK_FLG)
	and	a
	jr	nz,loop1$		; item shinka check shika shinai

	ld	a,b
	cp	1			; tuujou shinka
	jr	z,tuujou_shinka$

tuushin_shinka$:
	ld	a,(tuushin_flg)
	cp	50
	jp	nz,inc2_hl$		; tuushin koukan chuudenai

	ld	a,(hli)
	ld	b,a
	ld	a,(monsdata_dmy + 33)	; level
	cp	b
	jp	c,loop1$		; shinka shinai

;	jr	do_shinka1$
	jr	do_shinka$
	
item_shinka$:
	ld	a,(fighting_flg)	; debug 98/06/06 ピカチュウが勝手に進化してしまうバグ
	and	a			; debug 98/06/06
	ld	a,(hli)
	jp	nz,inc2_hl$		; debug 98/06/06

	ld	b,a
	ld	a,(sel_item_no)
	cp	b
	jp	nz,inc2_hl$		; item ga chigau
	
tuujou_shinka$:
	ld	a,(hli)
	ld	b,a
	ld	a,(monsdata_dmy + 33)	; level
	cp	b
	jp	c,inc_hl$		; shinka shinai

do_shinka$:
	ld	(mons_level),a
;	ld	a,(shinka_flg)
;	and	a
;	jr	z,do_shinka2$

do_shinka1$:
	ld	a,1
	ld	(shinka_flg),a

do_shinka2$:
	push	hl			; shinka data tbl address save
	ld	a,(hl)
	ld	(yes_no_map + 1),a

	ld	a,(sel_item_pos)
	ld	hl,my_cap_name
	call	get_pet_name
	call	str_cpy

	ld	hl,shinka_msg100$
	call	put_win_msg		; OYA? 
	ld	c,50
	call	wait_vb_s

	xor	a
	ld	(all_put_req),a
	S_POS	0,0
	E_POS	20,12
	call	block_cls	
	ld	a,1
	ld	(all_put_req),a

	ld	a,0ffh
	ld	(oam_flg),a
	call	oam_clr

;	call	shinka_animetion
	ld	hl,shinka_animetion
	ld	b,017h				;bank1e->bank17へ移動（村川）
	call	bank_push_call

;	push	af			; flg save
;	ld	a,(sel_item_pos)
;	ld	hl,my_cap_name
;	call	get_pet_name
;	call	str_cpy
;	pop	af			; flg load

	jp	c,shinka_cancel$	

	ld	hl,shinka_msg1$
	call	put_win_msg		; "ooooo ha" (use str_buf)

	pop	hl			; shinka data tbl address load
	ld	a,(hl)			; new monster number
	ld	(tbl_pos),a
	ld	(monsdata_dmy),a

	ld	(yes_no_map + 1),a

	ld	a,ASGN_MONS_NAME
	ld	(tbl_number),a
	ld	a,MONS_DATA_BANK
	ld	(tbl_bank),a
	call	get_table
	push	hl			; shinka data tbl address save

;	ld	hl,table_data
;	ld	bc,MONS_NAME_LEN
;	call	block_move		; set the new monster name

	ld	hl,shinka_msg2$
	call	put_nowin_msg

	ld	a,musfanfare52
	call	se_play
	call	se_wait

	ld	c,40
	call	wait_vb_s

	call	dvram_cls

	call	chk_pet_name$

	ld	a,(in_dat)
	push	af

	ld	a,(tbl_pos)		; new monster number
	ld	(in_dat),a
	ld	a,B_GET_ORDER_NO
	call	bank2bank
	ld	a,(in_dat)

	dec	a
	ld	hl,mons_tbl		; banke
	ld	bc,MONS_TBL_SIZE
	call	mul_any

;	ld	bc,MONS_TBL_SIZE
	ld	de,mons_data
	call	block_move		; set mons_data

	ld	a,(tbl_pos)		; new monster number
	ld	(mons_data),a

	pop	af
	ld	(in_dat),a

	ld	hl,monsdata_dmy + 16	; keikenchi address -1
	ld	de,monsdata_dmy + 34	; status address
	ld	b,1
	call	set_status_all		; set new status
	
	ld	a,(sel_item_pos)
	ld	hl,my_cap_data
	ld	bc,CAPDATA_LEN
	call	mul_any
	
	ld	e,l
	ld	d,h

	push	hl
	push	bc			; CAPDATA_LEN
	ld	bc,34
	add	hl,bc			; maxHP pos
	ld	a,(hli)
	ld	b,a
	ld	c,(hl)
	ld	hl,monsdata_dmy + 35	; maxHP pos
	ld	a,(hld)
	sub	c
	ld	c,a
	ld	a,(hl)
	sbc	a,b
	ld	b,a
	ld	hl,monsdata_dmy + 2	; nowHP pos
	ld	a,(hl)
	add	a,c
	ld	(hld),a
	ld	a,(hl)
	adc	a,b
	ld	(hl),a			; nowHP = nowHP + HP zouka bun

;	ld	hl,monsdata_dmy
	dec	hl
;	ld	bc,CAPDATA_LEN
	pop	bc			; CAPDATA_LEN
	call	block_move		; monsdata_dmy -> my_cap_data

	ld	a,(tbl_pos)			; new monster number
	ld	(in_dat),a

	xor	a
	ld	(my_or_gein),a

	call	waza_learn_chk

	pop	hl
;	call	zokusei_harituke
	ld	a,66
	call	bank2bank

	ld	a,(fighting_flg)
	and	a
	call	z,map_rewrite_chk

	ld	a,B_GET_ORDER_NO
	call	bank2bank
	ld	a,(in_dat)
	dec	a
	ld	c,a
	ld	b,1			; bit on
	ld	hl,zukan_flg
	push	bc
	call	call_b_ctrl		; kaijyuu zukan check
	pop	bc
	ld	hl,zukan_flg2
	call	call_b_ctrl		; kaijyuu zukan check
	

;	pop	hl			; shinka data tbl address load
;	ld	e,l
;	ld	d,h
	pop	de			; hl -> de shinka data tbl address load
	pop	hl			; my_cap_tbl load
	ld	a,(monsdata_dmy)	; new monster number
	ld	(hl),a
	push	hl			; my_cap_tbl save
	ld	l,e
	ld	h,d
	jr	inc_hl$
inc2_hl$:
	inc	hl
inc_hl$:
	inc	hl
	jp	loop2$

ret$:
	pop	de
	pop	bc
	pop	hl
	pop	af	
	ld	(wave_flg),a

	ld	a,(tuushin_flg)
	cp	50
	ret	z

	ld	a,(fighting_flg)
	and	a
	ret	nz
	
	ld	a,(shinka_flg)
	and	a
	call	nz,set_now_music
	ret

chk_pet_name$:
	ld	a,(tbl_pos)
	push	af

	ld	a,(mons_data)		; shinka mae no monster No
	ld	(tbl_pos),a
	call	get_table

	pop	af
	ld	(tbl_pos),a

	ld	hl,table_data
	ld	de,str_buf
chk_pet_name_lp$:
	ld	a,(de)
	inc	de
	cp	(hl)
	inc	hl
;	jr	nz,chk_pet_name_ret$
	ret	nz
	
	cp	050h
	jr	nz,chk_pet_name_lp$

	ld	a,(sel_item_pos)
	ld	bc,MONS_NAME_LEN
	ld	hl,my_cap_name
	call	mul_any

;	ld	d,h
;	ld	e,l
;	push	de
	push	hl			; hl -> de

	call	get_table

	ld	hl,table_data
	pop	de			; hl -> de
;	ld	bc,MONS_NAME_LEN

	jp	block_move

;chk_pet_name_ret$:
;	ret

shinka_cancel$:
	ld	hl,cancel_msg1$
	call	put_win_msg
	call	dvram_cls

	pop	hl

	call	map_rewrite_chk

	jp	loop1$

