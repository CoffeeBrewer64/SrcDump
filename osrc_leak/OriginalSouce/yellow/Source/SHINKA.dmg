cancel_msg1$:
	extern	cancel_msg1_2_SHINKA
	db I_MSG2	; mvmsg追加
	dw cancel_msg1_2_SHINKA	; mvmsg追加
	db 029h	; mvmsg追加
	db EOM	; put_msg_s終了コード mvmsg追加

shinka_msg100$:
	extern	shinka_msg100_3_SHINKA
	db I_MSG2	; mvmsg追加
	dw shinka_msg100_3_SHINKA	; mvmsg追加
	db 02ah	; mvmsg追加
	db EOM	; put_msg_s終了コード mvmsg追加

map_rewrite_chk:
	ld	a,(tuushin_flg)
	cp	50
	ret	z

	jp	map_rewrite2


;************************************************
; waza_learn_chk				*
;	in  :  in_dat				*
;	       sel_item_pos			*
;	       mons_level			*
;	       my_or_gein			*
;************************************************
waza_learn_chk:
	ld	a,(in_dat)		; monster number
	ld	(sel_item_no),a

	call	get_waza_hedder

level_check$:
	ld	a,(hli)
	and	a			; 00 = end mark
	jr	z,ret$

	ld	b,a
	ld	a,(mons_level)
	cp	b
	ld	a,(hli)			; oboeru waza number
	jr	nz,level_check$

	ld	d,a

	ld	a,(my_or_gein)
	and	a
;	ld	hl,benriya_mons + 8	; waza possition
	jr	nz,old_chk$

	ld	hl,my_cap_data + 8	; waza possition
	ld	a,(sel_item_pos)
	ld	bc,CAPDATA_LEN
	call	mul_any

old_chk$:
	ld	b,4
old_chk_loop$:
	ld	a,(hli)
	cp	d
	jr	z,ret$			; sudeni oboete iru
	
	dec	b
	jr	nz,old_chk_loop$

	ld	a,d
	ld	(waza_swap + 4),a

	ld	(in_dat),a
	call	get_waza_name
	call	str_cpy

	ld	a,B_ADD_WAZA1
	call	bank2bank
	ld	a,b
	and	a
	jr	z,ret$			; chancel
	
	ld	hl,check_pika
	ld	b,G_BANK3f
	call	bank_push_call
	jr	nc,ret$
	
	ld	a,(waza_swap + 4)	; waza No
	cp	85			; １０万ボルト
	jr	z,pika_10man$
	cp	87			; かみなり
	jr	nz,ret$
	
pika_10man$:
	ld	a,5			; １０万ボルトをするピカチュウ
	ld	(pika_friendly3),a
	ld	a,128 + 5		; ５歩継続
	ld	(pika_friendly + 1),a

ret$:
	ld	a,(sel_item_no)
	ld	(in_dat),a

	ret

;************************************************
; waza_capa_chk					*
;	in  :  in_dat		= waza No	*
;	       sel_item_no	= monster No	*
;	       mons_level			*
;	out :  Cy = ok  NC = ng			*
;************************************************
waza_capa_chk:
	ld	a,(sel_item_no)
	push	af

	call	waza_check_main$
	jr	c,capa_ok$

	call	shinkamae_chk
	jr	nc,capa_ng$			; shinkamae inai

	call	waza_check_main$
	jr	c,capa_ok$

	call	shinkamae_chk
	jr	nc,capa_ng$			; shinkamae inai

	call	waza_check_main$
	jr	c,capa_ok$

capa_ng$:
	pop	af
	ld	(sel_item_no),a
	and	a
	ret

capa_ok$:
	pop	af
	ld	(sel_item_no),a
	scf
	ret



waza_check_main$:
	ld	a,(in_dat)
	ld	(waza_swap + 4),a
	ld	a,B_CHECK_WAZA_FLG
	call	bank2bank

	ld	a,c
	and	a
	jr	nz,skill_ok$

	ld	hl,no_encount_tbl$
	ld	a,(sel_item_no)
	ld	de,1
	call	table_search
	jr	c,waza_oboe_chk$

	ld	a,0ffh				; table end 
	ld	(mons_data + 19),a
	ld	a,(in_dat)			; waza No
	ld	hl,mons_data + 15
	ld	de,1
	call	table_search
	jr	c,skill_ok$

waza_oboe_chk$:
	ld	a,(in_dat)
	ld	d,a				; waza No
	call	get_waza_hedder

loop1$:
	ld	a,(hli)
	and	a
	jr	z,skill_ng$

	ld	b,a
	ld	a,(mons_level)
	cp	b
	jr	c,skill_ng$
	
	ld	a,(hli)
	cp	d
	jr	z,skill_ok$

	jr	loop1$

skill_ng$:
	and	a
	ret

skill_ok$:
	scf
	ret


no_encount_tbl$:
	db	007,009,010,014,016,020,022,028,049,066,083,089,091
	db	099,100,103,104,105,114,125,126,139,142,149,152,154
	db	155,178,179,180,187,190
	db	0ffh


;************************************************
;	shinkamae_chk				*
;  IN  : sel_item_no = monster No		*
;  OUT : Cy = shinka mae found  NC = not found	*
;        sel_item_no = shinka mae No		*
;************************************************
shinkamae_chk:
	ld	c,0				; monster No counter ( -1 )
mons_loop$:
	ld	hl,shinka_tbl
	ld	b,0
	add	hl,bc
	add	hl,bc
	ld	a,(hli)
	ld	h,(hl)
	ld	l,a

once_more$:
	ld	a,(hli)
	and	a
	jr	z,next$				; shinka nashi
	cp	2				; item shinka
	jr	nz,mons_check$

	inc	hl
mons_check$:
	inc	hl				; level pass
	ld	a,(sel_item_no)
	cp	(hl)
	jr	z,find$				; monster No same

	inc	hl
	ld	a,(hl)
	and	a
	jr	nz,once_more$			; for eevui

next$:
	inc	c
	ld	a,c
	cp	190
	jr	c,mons_loop$

not_find$:
	and	a
	ret

find$:
	inc	c
	ld	a,c
	ld	(sel_item_no),a
	scf	
	ret
	


;************************************************
; waza_set					*
;	de = set address			*
;	sel_item_no = monster No		*
;	yes_no_map  = set mode			*
;	mons_level  = monster level		*
;		      0 = normal 1 = set point	*
;************************************************
waza_set:
	call	ready2ready
	push	hl
	push	de
	push	bc
	call	get_waza_hedder

	jr	waza_chk_loop$
	
waza_chk_loop0$:
	pop	de
waza_chk_loop01$:
	inc	hl
waza_chk_loop$:
	ld	a,(hli)
	and	a
	jp	z,ret$

	ld	b,a

	ld	a,(mons_level)
	cp	b
	jp	c,ret$

	ld	a,(yes_no_map)
	and	a
	jr	z,pass2$

	ld	a,(usr_buf)		; old level
	cp	b
	jr	nc,waza_chk_loop01$

pass2$:
	push	de

	ld	c,4
waza_add_loop1$:
	ld	a,(de)
	inc	de
	cp	(hl)
	jr	z,waza_chk_loop0$	; waza ga onaji
	dec	c
	jr	nz,waza_add_loop1$

	pop	de
	push	de

	ld	c,4
waza_add_loop2$:
	ld	a,(de)
	and	a
	jr	z,do$

	inc	de
	dec	c
	jr	nz,waza_add_loop2$

	pop	de
	push	de

	push	hl			; waza ga full nanode ichiban ue
	
	ld	h,d		
	ld	l,e
	call	oshidashi$

	ld	a,(yes_no_map)		; set mode
	and	a
	jr	z,oshidashi_end$

	push	de
	ld	bc,18
	add	hl,bc
	ld	d,h
	ld	e,l
	call	oshidashi$
	pop	de

oshidashi_end$:
	pop	hl

do$:
	ld	a,(hl)
	ld	(de),a

	ld	a,(yes_no_map)		; set mode
	and	a
	jr	z,waza_chk_loop0$

	push	hl

	ld	a,(hl)
	ld	hl,21
	add	hl,de
	push	hl

	dec	a
	ld	hl,waza_tbl
	ld	bc,6
	call	mul_any
	ld	de,yes_no_map
	ld	a,0eh
	call	bank_chg_block_m
	ld	a,(yes_no_map + 5)

	pop	hl
	ld	(hl),a
	
	pop	hl
	jr	waza_chk_loop0$

ret$:
	pop	bc
	pop	de
	pop	hl
	ret

oshidashi$:
	ld	c,3
oshidashi_loop$:
	inc	de
	ld	a,(de)
	ld	(hli),a
	dec	c
	jr	nz,oshidashi_loop$
	ret


call_b_ctrl:
	ld	a,B_BIT_CONTROL
	jp	bank2bank


get_waza_hedder:
	ld	hl,shinka_tbl	
	ld	b,0
	ld	a,(sel_item_no)		; monster No
	dec	a
	ld	c,a
	add	hl,bc
	add	hl,bc
	ld	a,(hli)
	ld	h,(hl)
	ld	l,a
shinka_pass_loop$:
	ld	a,(hli)
	and	a
	jr	nz,shinka_pass_loop$
	
	ret


