	pop	af
	dec	a
	jr	nz,sp2_calc_loop$

	E_HL	ACTOR_Y
	ld	a,(hl)
	sub	10h
	ld	d,a
	E_HL	ACTOR_X
	ld	e,(hl)

	ld	a,Bonus500ActNo
