RollingBonusUp:
	ld	a,(ROLLING_COUNT)	;回転数（ＭＡＸ３回）
	inc	a
	cp	4
	jr	c,pass$
	ld	a,3
pass$:
	ld	(ROLLING_COUNT),a
	ret

RollingBonus:
	ld	a,(ROLLING_COUNT)
	and	a
	ret	z

	ld	a,(ROLLING_STATUS)
	and	00000011b
	cp	00000011b		;両方向に回転したか？
	jr	z,special$	

	ld	a,(ROLLING_COUNT)	;かっこよさプラス幅の計算
	ld	d,a
	ld	e,01h
	ld	a,00h
count_loop$:
	add	a,e
	sla	e
	dec	d
	jr	nz,count_loop$

calc_loop$:
	push	af

	ld	e,50h
	call	CalcBonusScore
 
	pop	af
	dec	a
	jr	nz,calc_loop$
	
	E_HL	ACTOR_Y
	ld	a,(hl)
	sub	10h
	ld	d,a
	E_HL	ACTOR_X
	ld	e,(hl)

	ld	a,(ROLLING_COUNT)
	add	a,Bonus50ActNo-1
	push	bc
	call	EXAddActor
	pop	bc
	ret

special$:
	ld	a,(ROLLING_COUNT)
	cp	3			;３回転以上？
	jr	c,special1$
special2$
	ld	a,10			;かっこよさ＋５００
