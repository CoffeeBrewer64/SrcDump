;---------------------------------------;
;	ジャンプ軌道算出
;---------------------------------------;
HoubutuCalcInit:
	xor	a
	ld	(NAMIPIKA_WORK+1),a
	ld	(NAMIPIKA_WORK+2),a
	ret

HoubutuCalc:
	ld	a,(NAMIPIKA_WORK+1)
	and	a
	jr	nz,down$
 
	ld	a,(NAMIPIKA_WORK+0)		;タイミングカウンターＨＩＧＨ
	ld	d,a
	ld	a,(NAMIPIKA_WORK+2)		;タイミングカウンターＬＯＷ
	or	d
	jr	z,top$

	ld	a,(NAMIPIKA_WORK+2)
	ld	e,a
	ld	hl,-80h
	add	hl,de
	ld	a,l
	ld	(NAMIPIKA_WORK+2),a
	ld	a,h
	ld	(NAMIPIKA_WORK+0),a
 
	ld	e,a
	ld	d,00h
	call	mulcalc
	ld	e,l
	ld	d,h
	ld	a,4
	call	mulcalc
	ld	a,l
	xor	11111111b
	inc	a
	ld	l,a
	ld	a,h
	xor	11111111b
	ld	h,a

	push	hl
	E_HL	ACTOR_Y
	ld	d,(hl)
	E_HL	ACTOR_WK1
	ld	e,(hl)
	pop	hl

	add	hl,de
	ld	e,l
	ld	d,h

	E_HL	ACTOR_Y
	ld	(hl),d
	E_HL	ACTOR_WK1
	ld	(hl),e
	and	a
	ret
top$:
	ld	a,1
	ld	(NAMIPIKA_WORK+1),a
	and	a
	ret
down$:
	ld	a,(WAVE_Y)
	ld	e,a
	E_HL	ACTOR_Y
	ld	a,(hl)
	cp	090h
	jr	nc,calc$
	cp	e
	jr	nc,reset$
calc$:
	ld	a,(NAMIPIKA_WORK+0)
	ld	d,a
	ld	a,(NAMIPIKA_WORK+2)
	ld	e,a
	ld	hl,80h
	add	hl,de

	ld	a,l
	ld	(NAMIPIKA_WORK+2),a
	ld	a,h
	ld	(NAMIPIKA_WORK+0),a
 
	ld	e,a
	ld	d,00h
	call	mulcalc
	ld	e,l
	ld	d,h
	ld	a,4
	call	mulcalc

	push	hl
	E_HL	ACTOR_Y
	ld	d,(hl)
	E_HL	ACTOR_WK1
	ld	e,(hl)
	pop	hl

	add	hl,de
	ld	e,l
	ld	d,h

	E_HL	ACTOR_Y
	ld	(hl),d
	E_HL	ACTOR_WK1
	ld	(hl),e
	and	a
	ret
reset$:
	E_HL	ACTOR_Y
	ld	a,(WAVE_Y)
	ld	(hl),a
	E_HL	ACTOR_WK1
	ld	(hl),00h
	scf
	ret
	
mulcalc:
	ld	hl,000h
calcloop1$:
	srl	a
	jr	nc,calcloop2$
	add	hl,de
calcloop2$:
	sla	e
	rl	d
	and	a
	jr	nz,calcloop1$
	ret

