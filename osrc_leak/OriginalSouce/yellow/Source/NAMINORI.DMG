E_HL	macro	address
	ld	hl,address
	add	hl,bc
	endm
;---------------------------------------
ef_initequ	macro
mcount	=	0	
	endm

ef_equ		macro	mequno
mequno	equ	mcount
mcount	=	mcount + 1
	endm

ef_equ_g	macro	mequno
mequno	equ	mcount
mcount	=	mcount + 1
	public	mequno
	endm
;---------------------------------------
ef_initndb	macro
mcount	=	0	
	endm

ef_ndb		macro	mdata,mequno
	db	mdata
mequno	equ	mcount
mcount	=	mcount + 1
	endm
;---------------------------------------
ef_initndw	macro
mcount	=	0	
	endm

ef_initndw2	macro	mdata
mcount	=	mdata	
	endm

ef_inc_ndw	macro	maddress
	dw	maddress
mcount	=	mcount + 1
	endm

ef_ndw_g	macro	maddress,mequno
	dw	maddress
mequno	equ	mcount
mcount	=	mcount + 1
	public	mequno
	endm

ef_ndw		macro	maddress,mequno
	dw	maddress
mequno	equ	mcount
mcount	=	mcount + 1
	endm

;=======================================;
;	ＲＡＭ定義
;=======================================;
NAMINORI_WORK_TOP	equ	dmy_map+200

MAIN_SEQ		equ	1+NAMINORI_WORK_TOP	;メイン シーケンスナンバー	
PIKA_SEQ		equ	1+MAIN_SEQ		;ピカチュウ シーケンスナンバー	
MAP_SEQ			equ	1+PIKA_SEQ		;マップ表示 シーケンスナンバー	

VBLANK_TIMING		equ	1+MAP_SEQ		;ＳＧＢ／ＢＧのＶＢＬＡＮＫ周期用ワーク

WAVE_KIND		equ	1+VBLANK_TIMING		;波の種類
TIMER_COUNT		equ	1+WAVE_KIND		;持ちタイム
ROLLING_COUNT		equ	3+TIMER_COUNT		;空中回転数	
BONUS_COUNT		equ	1+ROLLING_COUNT		;かっこよさ
TOTAL_SCORE		equ	2+BONUS_COUNT		;総合得点

PIKA_ANM_OFFSET		equ	2+TOTAL_SCORE		;揺れによるピカチュウのアニメオフセット	
PIKA_ANM_DIRECT		equ	1+PIKA_ANM_OFFSET	;揺れの方向	
PIKA_ANM_WORK		equ	1+PIKA_ANM_DIRECT	;揺れの周期用ワーク

DEAD_WORK		equ	1+PIKA_ANM_WORK		;死にウェイト
PAUSE_FLAG		equ	1+DEAD_WORK		;一時停止フラグ
SPEED_COUNT		equ	1+PAUSE_FLAG		;現在スピード
TOTAL_LENGTH		equ	2+SPEED_COUNT		;進行距離
HIT_POINT		equ	3+TOTAL_LENGTH		;当たり判定取得用ワーク	
WAVE_Y			equ	2+HIT_POINT		;波の高さ		
SIBUKI_TIMER		equ	1+WAVE_Y		;水しぶきの表示周期用ワーク
NAMIPIKA_WORK		equ	1+SIBUKI_TIMER		;ジャンプ軌道計算用ワーク
FLOOR_CHR		equ	3+NAMIPIKA_WORK		;アトリビュートワーク
SCROLL_WORK		equ	40+FLOOR_CHR		;ＢＧスクロール用ワーク		
HITCHECK_BUFFER		equ	3+SCROLL_WORK		;当たり判定用ワーク
MAP_WRITE_POS		equ	20+HITCHECK_BUFFER	;マップ表示位置
ROLLING_STATUS		equ	1+MAP_WRITE_POS		;空中回転ステータス
TIMEOVER_FLAG		equ	1+ROLLING_STATUS	;タイムオーバー処理用フラグ
TIMEOVER_WORK		equ	1+TIMEOVER_FLAG		;タイムオーバー処理用ワーク
WAIT_WORK		equ	1+TIMEOVER_WORK		;メインシーケンス ウェイト用ワーク
TITLE_WORK		equ	1+WAIT_WORK		;タイトル表示時用ワーク
TEMPO_CHANGE_REQ	equ	1+TITLE_WORK		;ＢＧＭテンポ変更リクエスト
ACTSPEED_X		equ	1+TEMPO_CHANGE_REQ	;雲スクロール用ワーク

NAMINORI_WORK_END	equ	2+ACTSPEED_X



;=======================================;
;
;	サブゲームの 呼び出し
;
;=======================================;
NaminoriPikatyu:
	call	PaletteOFF		;パレットオフ（画面は白）
	call	wait_vb
	call	wait_vb
	call	wait_vb

	ld	a,(wave_flg)
	push	af
	xor	a
	ld	(wave_flg),a
	ld	a,(oam_flg)
	push	af
	ld	a,0ffh
	ld	(oam_flg),a

	ld	a,(IE)
	push	af
	xor	a
	ld	(IFL),a
	ld	a,00001111b		;割り込み制御
	ld	(IE),a
	ld	a,00001000b
	ld	(STAT),a

	ld	a,(all_putadr_h)
	push	af
	ld	a,98h
	ld	(all_putadr_h),a

	call	NamiPikaTitle
	call	NamiPikaSubGame

	xor	a
	ld	(BGP),a
	ld	(OBP0),a
	ld	(OBP1),a
  ifn	pm_cgb
	call	change_cgb_bgp
	call	change_cgb_obp0
	call	change_cgb_obp1
  endif

	call	EXActorInit		;アクター終了イニシャライズ		
	call	oam_clr			;ＯＡＭクリア
	xor	a
	ld	(LasterReq),a
	ld	(headerx),a
	ld	(headery),a
	ld	a,90h
	ld	(window_y),a
	call	wait_vb

	pop	af
	ld	(all_putadr_h),a
	xor	a
	ld	(IFL),a
	pop	af
	ld	(IE),a
	xor	a
	ld	(STAT),a

	call	color_rewrite		;ＳＧＢカラー再セット
	call	MapDataAllSet		;マップデータ（ＢＧ＆ＯＢＪ）再セット
	call	set_now_music		;ＢＧＭ再セット
	call	palset			;フィールド画面でのＢＧ＆ＯＢＪパレットセット
	pop	af
	ld	(oam_flg),a
	pop	af
	ld	(wave_flg),a
	ret





;=======================================;
;
;	基幹ループ
;
;=======================================;
NamiPikaSubGame:
	call	NamiPikaInit		;イニシャライズ
	call	wait_vb

 ifn  ASSEMBLE__ENGLISH			; 変更したタイトル画面"Pikachu's Beach"
	ld	b,<SGBnamipikaNo	; のアトリビュートを元に戻す
	call	color_set		;ＳＧＢ用カラーセット
 endif

mainloop$:
	ld	a,(MAIN_SEQ)
	bit	7,a			;ゲーム終了フラグ
	ret	nz

	call	KeyStatus		;キーステータス取得

	call	BreakCheck		;ゲーム中止チェック
	ret	nz
	
;	call	PauseCheck		;ゲーム中断チェック
;	ld	a,(PAUSE_FLAG)
;	and	a			;０＝継続、１＝中断
;	jr	nz,in_pause$

	call	MainSeqence		;メインシーケンス

	ld	a,< 15*4+oam_buf	;１５キャラ以降のＯＡＭをアクターで使用
	ld	(OAM_Address),a
	call	EXActorJump		;アクター処理

	call	MoveSky
in_pause$:
	call	VblankWait

	call	MusicTempoUpDown
	jr	mainloop$

;---------------------------------------;
;	ＶＢＬＡＮＫ周期補正
;---------------------------------------;
VblankWait:
	call	wait_vb			;ＶＢＬＡＮＫウェイト
 ifn 0
	ld	a,(sgb_flg)		;ＳＧＢ用周期
	and	a
	ret	nz

	ld	a,(MAIN_SEQ)
	cp	GameMainNo
	jr	z,check$
	cp	GameTimeOverNo
	ret	nz
check$:
	ld	a,(VBLANK_TIMING)
	xor	1
	ld	(VBLANK_TIMING),a
	ret	nz

	call	wait_vb	
 endif
	ret

;---------------------------------------;
;	ゲーム中止チェック
;---------------------------------------;
BreakCheck:
	ld	hl,yellow_event+00h	;波乗りゲームを遊んだか？フラグ
	bit	1,(hl)			;遊んでなければ中止できない	
	ret	z

	ld	a,(joy_on)
	and	BT_SE			;セレクトボタンで中止
	ret

;---------------------------------------;
;	ゲーム中断チェック
;---------------------------------------;
PauseCheck:
	ld	a,(joy_on)		;スタートボタンで中断＆継続
	and	BT_ST
	ret	z

	ld	hl,PAUSE_FLAG
	ld	a,(hl)
	xor	1			;ボタンを押すたびに 中断、継続の切り替え
	ld	(hl),a
	ret

;---------------------------------------;
;	音楽テンポ変更
;---------------------------------------;
MusicTempoUpDown:
	ld	a,(TEMPO_CHANGE_REQ)
	and	a
	ret	z

	ld	hl,sound_len
	ld	a,1
	cp	(hl)
	ret	nz
	inc	hl
	cp	(hl)
	ret	nz
	inc	hl
	cp	(hl)
	ret	nz

	ld	a,(SPEED_COUNT+0)
	ld	e,a
	ld	a,(SPEED_COUNT+1)
	and	00000011b
	ld	d,a
	sla	e
	rl	d
	ld	e,d
	ld	d,00h
	ld	hl,TempoTable$
	add	hl,de
	add	hl,de
	ld	a,(hli)
	ld	(bgm_tempo+1),a
	ld	a,(hl)
	ld	(bgm_tempo+0),a
	ret
TempoTable$:
	dw	0075h
	dw	006dh
	dw	0065h
	dw	005dh
	dw	0055h
	
MusicTempoRecover:
	ld	hl,sound_len
	ld	a,1
	cp	(hl)
	ret	nz
	inc	hl
	cp	(hl)
	ret	nz
	inc	hl
	cp	(hl)
	ret	nz

	ld	a,75h
	ld	(bgm_tempo+1),a
	xor	a
	ld	(bgm_tempo+0),a
	ret





