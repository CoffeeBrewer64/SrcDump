NamiPikaInit:
	call	DVRAMcls		;ダミーＶＲＡＭのクリア
	call	oam_clr

	call	lcdc_stop

	ld	hl,NAMINORI_WORK_TOP
	ld	bc,NAMINORI_WORK_END - NAMINORI_WORK_TOP
	xor	a
	call	memset			;ワークＲＡＭエリアのクリア

	ld	hl,LasterBuffer
	ld	bc,200h
	xor	a
	call	memset			;ラスターバッファのクリア

	xor	a
	ld	(all_put_req),a
	call	EXActorInit		;アクターのイニシャライズ

	ld	hl,NaminoriBG_CGX
	ld	de,9000h
	ld	bc,0500h
	ld	a,G_BANK20
	call	chrset			;ＢＧキャラデータ セット

	ld	hl,NaminoriOBJ_CGX
	ld	de,8000h
	ld	bc,1000h
	ld	a,G_BANK20
	call	chrset			;ＯＢＪキャラデータ セット
 
	ld	a,<HeaderTable		;アクターヘッダーアドレスをセット
	ld	(A_Header+0),a
	ld	a,>HeaderTable
	ld	(A_Header+1),a

	ld	a,<MoveTable		;アクター動作アドレスをセット
	ld	(A_Move+0),a
	ld	a,>MoveTable
	ld	(A_Move+1),a

	ld	a,<PatTable		;アクターＯＢＪ構成パターンアドレスをセット
	ld	(A_Pat+0),a
	ld	a,>PatTable
	ld	(A_Pat+1),a

	ld	a,<AnmTable		;アクターアニメパターンアドレスをセット
	ld	(A_Animetion+0),a
	ld	a,>AnmTable
	ld	(A_Animetion+1),a

	ld	hl,9800h		;ＢＧマップ（空＆海）セット
	ld	bc,800h
	ld	a,0
	call	memset
	ld	hl,20h*6+9800h
	ld	bc,20h*12
	ld	a,0bh
	call	memset

	ld	a,<NamiPikaActNo	;ピカチュウ セット
	ld	de,7458h
	call	EXAddActor
	ld	a,74h
	ld	(WAVE_Y),a

	call	LasterSinSet		;ラスターデータ（波用）セット

	xor	a
	ld	(headerx),a		;スクロールレジスタ（Ｘ）初期設定
	ld	(headery),a		;スクロールレジスタ（Ｙ）初期設定
	ld	a,7eh			;ステータス用ウインドウ位置セット
	ld	(window_y),a
	ld	a,SCY
	ld	(LasterReq),a		;ラスター（Ｙ方向）のリクエスト

	ld	a,40h
	ld	(SPEED_COUNT+0),a	;初期スピード（＝０）セット
	xor	a
	ld	(SPEED_COUNT+1),a

;	xor	a
;	ld	(TIMER_COUNT+0),a	;持ちタイムのセット
;	ld	(TIMER_COUNT+1),a
;	ld	a,2
;	ld	(TIMER_COUNT+2),a
	xor	a
	ld	(TIMER_COUNT+0),a	;持ちタイムのセット
	ld	a,60h
	ld	(TIMER_COUNT+1),a

	ld	hl,HITCHECK_BUFFER	;初期当たり判定セット
	ld	bc,20
	ld	a,074h
	call	memset

	call	StatusOBJset		;ステータス表示ＯＢＪセット
	call	StatusBGset		;ステータス表示ＢＧセット

; ifn  ASSEMBLE__ENGLISH		; 変更したタイトル画面"Pikachu's Beach"
;	ld	b,<SGBnamipikaNo	; のアトリビュートを元に戻す
;	call	color_set		;ＳＧＢ用カラーセット
; endif
	ld	a,11100011b
	ld	(LCDC),a

	call	BGPset
;	ld	a,11100100b		;パレットセット
;	ld	(BGP),a
	ld	a,11100100b
	ld	(OBP0),a
	ld	a,11100000b
	ld	(OBP1),a
  ifn	pm_cgb
	call	change_cgb_obp0
	call	change_cgb_obp1
  endif
	ret

BGPset:
	ld	a,(sgb_flg)
	and	a
	jr	nz,sgb$
	ld	a,11010000b
	ld	(BGP),a
  ifn	pm_cgb
	call	change_cgb_bgp
  endif
	ret
sgb$:
	ld	a,11100100b
	ld	(BGP),a
  ifn	pm_cgb
	call	change_cgb_bgp
  endif
	ret

;---------------------------------------;
;	ステータス用ＯＢＪ＆ＢＧセット
;---------------------------------------;
StatusOBJset:
	ld	hl,oam_buf
 ifn 0
	ld	de,TimerChrTable$	;タイム（分）
	ld	b,97h
	ld	c,68h
	ld	a,1
	call	set$
	ld	de,TimerChrTable$	;タイム（秒）
	ld	b,97h
	ld	c,78h
	ld	a,2
	call	set$
	ld	de,TimerChrTable$	;タイム
	ld	b,97h
	ld	c,90h
	ld	a,2
	call	set$
 endif
	ld	de,TimerChrTable$	;タイム（秒）
	ld	b,97h
	ld	c,80h
	ld	a,4
	call	set$

	ld	de,PosChrTable$		;進行距離マーカー
	ld	b,96h
;	ld	c,48h
	ld	c,50h
	ld	a,1
	call	set$

	ld	de,Sky1ChrTable$	;雲（１） セット
	ld	b,14h
	ld	c,20h
	ld	a,5
	call	set$

	ld	de,Sky2ChrTable$	;雲（２） セット
	ld	b,20h
	ld	c,80h
	ld	a,4
	call	set$
	ret
set$:
	push	af
	ld	(hl),b			;ＯＡＭ（Ｙ座標）
	inc	hl
	ld	(hl),c			;ＯＡＭ（Ｘ座標）
	inc	hl
	ld	a,(de)
	ld	(hl),a			;ＯＡＭ（キャラナンバー）
	inc	hl
	ld	(hl),0			;ＯＡＭ（アトリビュート）
	inc	hl

	ld	a,c
	add	a,8
	ld	c,a

	inc	de

	pop	af
	dec	a
	jr	nz,set$
	ret

PosChrTable$:
	db	0feh
TimerChrTable$:
	db	0d0h,0d0h,0d0h,0d0h
Sky1ChrTable$:
	db	0ech,0edh,0edh,0eeh,0efh
Sky2ChrTable$:
	db	0ech,0edh,0eeh,0efh

StatusBGset:
 ifn 0
	ld	de,20h+9c00h
	ld	hl,data$
	ld	c,11
loop$:
	ld	a,(hli)
	ld	(de),a
	inc	de
	dec	c
	jr	nz,loop$

;	ld	hl,20h+0dh+9c00h
;	ld	(hl),0fdh
;	ld	hl,20h+10h+9c00h
;	ld	(hl),0fdh
	ld	hl,20h+0dh+9c00h
	ld	(hl),0fdh
	ld	hl,20h+10h+9c00h
	ld	(hl),0fdh
	ret
data$:
	db	15h,16h,1ah,17h,17h,17h,17h,17h,1bh,18h,19h
 endif
	ld	de,20h+1+9c00h
	ld	hl,data$
	ld	c,9
loop$:
	ld	a,(hli)
	ld	(de),a
	inc	de
	dec	c
	jr	nz,loop$

	ld	hl,9c00h+1
	ld	(hl),015h
	ld	hl,9c00h+2
	ld	(hl),016h

	ld	hl,20h+12+9c00h
	ld	(hl),01bh
	ld	hl,20h+13+9c00h
	ld	(hl),01ch
	ret
data$:
	db	17h,18h,19h,19h,19h,19h,19h,19h,19h





;=======================================;
;
;	メインシーケンス
;
;=======================================;
MainSeqence:
	ld	a,(MAIN_SEQ)
	ld	e,a
	ld	d,00h
	ld	hl,SeqTable$
	add	hl,de
	add	hl,de
	ld	a,(hli)
	ld	h,(hl)
	ld	l,a
	jp	(hl)
SeqTable$:
	ef_initndw
	ef_inc_ndw	GameInit
	ef_ndw		GameMain,GameMainNo
	ef_inc_ndw	GameGoalInit
	ef_inc_ndw	GameGoal
	ef_inc_ndw	GameScoreCalcInit
	ef_inc_ndw	GameScorePutTime
	ef_inc_ndw	GameScorePutBonus
	ef_inc_ndw	GameScorePutTotal
	ef_inc_ndw	GameScoreCalcTime
	ef_inc_ndw	GameScoreCalcBonus
	ef_inc_ndw	GameScoreCalcEnd
	ef_ndw		GameEnd,GameEndNo
	ef_ndw		GameTimeOver,GameTimeOverNo
	
GameInit:
	ld	a,<StartActNo		;スタートサイン セット
	ld	de,48e0h
	call	EXAddActor

	ld	hl,MAIN_SEQ
	inc	(hl)

	ld	a,1
	ld	(TEMPO_CHANGE_REQ),a
	ret

GameMain:
	ld	a,(TOTAL_LENGTH)	;進行距離
	cp	18h			;終了地点
	jr	nc,next$

	ld	hl,TIMER_COUNT+0	;持ちタイム
	ld	a,(hli)
	or	(hl)
;	inc	hl
;	or	(hl)
	and	a			;終了
	jr	z,time_over$

	call	rnd
	ld	(WAVE_KIND),a		;次の波の大きさを選択（ランダム）
	call	LasterShift
	call	FloorCheck
	call	FloorChrSet
	call	BGScroll

	call	TotalLengthCalc		;進行距離の計算
	call	TimerDec
	call	TimerPut
	ret
next$:
	ld	hl,MAIN_SEQ
	inc	(hl)

;	ld	a,<GoalActNo		;ゴールサインセット
;	ld	de,48e0h
;	call	EXAddActor
	xor	a
	ld	(TEMPO_CHANGE_REQ),a

	ld	a,0c0h
	ld	(WAIT_WORK),a
	ret
time_over$:
	ld	a,1
	ld	(TIMEOVER_FLAG),a
	ld	a,GameTimeOverNo
	ld	(MAIN_SEQ),a
	ld	a,80h
	ld	(TIMEOVER_WORK),a

	ld	a,<TimeOverActNo	;タイムオーバーサイン セット
	ld	de,8858h
	call	EXAddActor
	E_HL	ACTOR_DY
	ld	(hl),-80h
	E_HL	ACTOR_WK0
	ld	(hl),80h
	E_HL	ACTOR_WK1
	ld	(hl),30h
	xor	a
	ld	(TEMPO_CHANGE_REQ),a
	ret

GameGoalInit:
	call	GoalWait
	jr	c,next$

	xor	a
	ld	(WAVE_KIND),a		;次の波の大きさを選択（ランダム）
	call	LasterShift
	call	FloorCheck
	call	FloorChrSet
	call	BGScroll2
	call	MusicTempoRecover
	ret
next$:
	ld	hl,MAIN_SEQ
	inc	(hl)

