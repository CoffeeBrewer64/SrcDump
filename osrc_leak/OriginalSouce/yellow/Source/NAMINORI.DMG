;	数の表示
;---------------------------------------;
CounterPut:
	ld	c,a
	swap	a
	and	00001111b
	add	a,0d0h
	ld	(hli),a
	ld	a,c
CounterPut2:
	and	00001111b
	add	a,0d0h
	ld	(hl),a
	dec	de
	ret

;---------------------------------------;
;	ＳＩＮ＆ＣＯＳの計算
;---------------------------------------;
IntCos:
	add	a,10h
IntSin:
	and	00111111b
	cp	20h
	jr	nc,minus$
plus$:
	call	SinCalc$
	ld	a,h
	ret
minus$:
	and	00011111b
	call	SinCalc$
	ld	a,h
	xor	11111111b
	inc	a
	ret

SinCalc$:
	ld	e,a
	ld	a,d
	ld	d,00h
	ld	hl,IntSinTable
	add	hl,de
	add	hl,de
	ld	e,(hl)
	inc	hl
	ld	d,(hl)
multi$:				; (i)a = A, (i)de = B, (i)l = float
	ld	hl,000h		; (o)hl = A*B
calcloop1$:
	srl	a
	jr	nc,calcloop2$
	add	hl,de
calcloop2$:
	sla	e
	rl	d
	and	a
	jr	nz,calcloop1$
	ret

