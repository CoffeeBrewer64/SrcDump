	ld	hl,TIMER_COUNT+0
	ld	e,98h
	ld	d,2
	call	calc$
	ret	nc

	inc	hl
	ld	e,59h
	ld	d,1
	call	calc$
	ret	nc

	inc	hl
	ld	e,59h
	ld	d,1
