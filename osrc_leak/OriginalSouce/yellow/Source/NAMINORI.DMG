;=======================================;
;
;	スクロール
;
;=======================================;
BGScroll2:
	ld	a,0a0h
	ld	(MAP_WRITE_POS),a

	ld	a,(headerx)
	ld	h,a
	ld	a,(SCROLL_WORK)		;スクロールＸ 少数値
	ld	l,a

	ld	de,900h
	add	hl,de			;スピード加算

	ld	a,l
	ld	(SCROLL_WORK),a
	ld	a,h
	ld	(headerx),a
	jr	BGScrollMap
BGScroll:
	ld	a,0a0h
	ld	(MAP_WRITE_POS),a

	ld	a,(headerx)
	ld	h,a
	ld	a,(SCROLL_WORK)		;スクロールＸ 少数値
	ld	l,a

	ld	de,180h
	add	hl,de			;スピード加算

	ld	a,l
	ld	(SCROLL_WORK),a
	ld	a,h
	ld	(headerx),a
BGScrollMap:
	ld	hl,SCROLL_WORK+1	;スクロールＸ値
	ld	a,(headerx)
	cp	(hl)
	ret	z			;Ｘ値が同じ時は次のマップを転送しない
	ld	(hl),a

	and	11110000b
	ld	hl,SCROLL_WORK+2	;書き換えＸ値
	cp	(hl)
	ret	z			;書き換えＸ値が同じ時は次のマップを転送しない
	ld	(hl),a
set$:
	call	MapSequence		;次のマップセルアドレスをセット
	ld	a,b
	ld	(HIT_POINT+0),a
	ld	a,c
	ld	(HIT_POINT+1),a

	push	de
	ld	hl,HITCHECK_BUFFER
	ld	de,HITCHECK_BUFFER+2
	ld	c,20-2
loop$:
	ld	a,(de)
	inc	de
	ld	(hli),a
	dec	c
	jr	nz,loop$
	ld	a,(HIT_POINT+0)
	ld	(hli),a
	ld	a,(HIT_POINT+1)
	ld	(hl),a
	pop	de

	ld	hl,haji_buf
	ld	c,8
setloop$:				;次のマップキャラをセット
	ld	a,(de)
	call	hajicellset		;セル → キャラ
	inc	de
	dec	c
	jr	nz,setloop$

	ld	a,(MAP_WRITE_POS)
	ld	e,a
	ld	a,(headerx)		
	add	a,e
	and	11110000b
	srl	a
	srl	a
	srl	a
	ld	e,a
	ld	d,00h
	ld	hl,9800h
	add	hl,de			;書き換えアドレスの算出
	ld	a,l
	ld	(haji_put_buf+1),a	;転送アドレス ＬＯＷ
	ld	a,h
	ld	(haji_put_buf+2),a	;転送アドレス ＨＩＧＨ
	ld	a,1
	ld	(haji_put_buf+0),a	;転送リクエスト
	ret

hajicellset:
	push	de
	push	hl
	ld	l,a
	ld	h,00h
	ld	de,NaminoriBG_Cell
	add	hl,hl
	add	hl,hl
	add	hl,de
	ld	e,l
	ld	d,h
	pop	hl

	ld	a,(de)
	inc	de
	ld	(hli),a
	ld	a,(de)
	inc	de
	ld	(hli),a
	ld	a,(de)
	inc	de
	ld	(hli),a
	ld	a,(de)
	inc	de
	ld	(hli),a
	pop	de
	ret

MapSequence:
	ld	a,(MAP_SEQ)
	ld	e,a
	ld	d,00h
	ld	hl,SeqTable$
	add	hl,de
	add	hl,de
