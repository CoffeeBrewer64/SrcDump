;---------------------------------------;
;	ベストスコアの比較
;---------------------------------------;
HiScoreCheck:
	ld	hl,high_score+1
	ld	a,(TOTAL_SCORE+1)
	cp	(hl)
	jr	c,ret$
	jr	nz,hiscore$

	dec	hl
	ld	a,(TOTAL_SCORE+0)
	cp	(hl)
	jr	c,ret$
	jr	nz,hiscore$
ret$:
	call	se_wait
	ld	e,27
	call	VoicePlay
	and	a
	ret
hiscore$:
	ld	a,(TOTAL_SCORE+0)
	ld	(high_score+0),a
	ld	a,(TOTAL_SCORE+1)
	ld	(high_score+1),a

	call	se_wait
	ld	e,33
	call	VoicePlay

	ld	a,<musnamifanfare4
	call	SEplay
	scf
	ret

