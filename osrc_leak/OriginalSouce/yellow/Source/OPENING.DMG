;=======================================;
;	MACRO
;=======================================;
E_HL	macro	address
	ld	hl,address
	add	hl,bc
	endm

;============================================
ef_initequ	macro
mcount	=	0	
	endm

ef_equ		macro	mequno
mequno	equ	mcount
mcount	=	mcount + 1
	endm

ef_equ_g	macro	mequno
mequno	equ	mcount
mcount	=	mcount + 1
	public	mequno
	endm

;============================================
ef_initndb	macro
mcount	=	0	
	endm

ef_ndb		macro	mdata,mequno
	db	mdata
mequno	equ	mcount
mcount	=	mcount + 1
	endm

;============================================
ef_initndw	macro
mcount	=	0	
	endm

ef_initndw2	macro	mdata
mcount	=	mdata	
	endm

ef_inc_ndw	macro	maddress
	dw	maddress
mcount	=	mcount + 1
	endm

ef_ndw_g	macro	maddress,mequno
	dw	maddress
mequno	equ	mcount
mcount	=	mcount + 1
	public	mequno
	endm

ef_ndw		macro	maddress,mequno
	dw	maddress
mequno	equ	mcount
mcount	=	mcount + 1
	endm

