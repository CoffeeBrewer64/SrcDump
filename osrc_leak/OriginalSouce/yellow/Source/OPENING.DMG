MainSeqence:
	ld	a,(seq_work)
	ld	hl,SeqTable$
	call	SeqAddressGet
	jp	(hl)
SeqTable$:
	ef_initndw
	ef_ndw		Scene1Init,Scene1No
	ef_inc_ndw	Scene1

	ef_ndw		Scene2Init,Scene2No
	ef_inc_ndw	Scene2

	ef_ndw		Scene3Init,Scene3No
	ef_inc_ndw	Scene3

	ef_ndw		Scene4Init,Scene4No
	ef_inc_ndw	Scene4

	ef_ndw		Scene5Init,Scene5No
	ef_inc_ndw	Scene5

	ef_ndw		Scene6Init,Scene6No
	ef_inc_ndw	Scene6

	ef_ndw		Scene7Init,Scene7No
	ef_inc_ndw	Scene7_1
	ef_inc_ndw	Scene7_2
	ef_inc_ndw	Scene7_3
	ef_inc_ndw	Scene7_4

	ef_ndw		DemoExit,DemoExitNo

SeqNoInc:
	ld	hl,seq_work
	inc	(hl)
	ret

