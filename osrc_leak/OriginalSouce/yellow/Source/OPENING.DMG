seq_work		equ	300+dmy_map

;=======================================;
;
;	MAINLOOP
;
;=======================================;
OpeningDemoYellow:
	ld	a,(IE)
	push	af
	xor	a
	ld	(IFL),a
	ld	a,00001111b
	ld	(IE),a
	ld	a,00001000b
	ld	(STAT),a

	call	OpeningDemoInit
	call	wait_vb
main_loop$:
	ld	a,(seq_work)
	bit	7,a
	jr	nz,exit$

	call	cont_repeat
	ld	a,(joy_on)
	and	BT_A+BT_B+BT_ST
	jr	nz,exit$
	
	call	MainSeqence

	ld	a,<oam_buf
	ld	(OAM_Address),a
	call	EXActorJump
	
  ifn	pm_cgb & pm_cgb_obj_atr
	ld	a,(seq_work)
	cp	07h
	call	z,set_namipika_attr$
	cp	0bh
	call	z,set_soratobu_attr$
  endif
	
	call	wait_vb
	jr	main_loop$
exit$:
	call	PalOff
	xor	a
	ld	(LasterReq),a
	call	wait_vb

	xor	a
	ld	(IFL),a
	pop	af
	ld	(IE),a

	ld	a,90h
	ld	(window_y),a
	call	EXActorInit
	ld	hl,dmy_vram
	ld	bc,20*18
	xor	a
	call	memset
	call	oam_clr

;	call	MusicStop

	ld	a,1
	ld	(all_put_req),a
	call	wait_vb
	call	wait_vb
	call	wait_vb
	xor	a
	ld	(all_put_req),a

;	call	palset
	ret

  ifn	pm_cgb & pm_cgb_obj_atr
set_namipika_attr$:
	ld	a,(4 * 8 + 3 + oam_buf)
	or	01h
	ld	(4 * 8 + 3 + oam_buf),a
	ld	a,(4 * 14 + 3 + oam_buf)
	or	01h
	ld	(4 * 14 + 3 + oam_buf),a
	ld	a,(4 * 16 + 3 + oam_buf)
	or	01h
	ld	(4 * 16 + 3 + oam_buf),a
	ld	a,(4 * 18 + 3 + oam_buf)
	or	01h
	ld	(4 * 18 + 3 + oam_buf),a
	ld	a,(4 * 19 + 3 + oam_buf)
	or	01h
	ld	(4 * 19 + 3 + oam_buf),a
	ret

set_soratobu_attr$:
	ld	a,(4 * 18 + 3 + oam_buf)
	or	01h
	ld	(4 * 18 + 3 + oam_buf),a
	ld	a,(4 * 19 + 3 + oam_buf)
	or	01h
	ld	(4 * 19 + 3 + oam_buf),a
	ld	a,(4 * 20 + 3 + oam_buf)
	or	01h
	ld	(4 * 20 + 3 + oam_buf),a
	ld	a,(4 * 25 + 3 + oam_buf)
	or	01h
	ld	(4 * 25 + 3 + oam_buf),a
	ld	a,(4 * 26 + 3 + oam_buf)
	or	01h
	ld	(4 * 26 + 3 + oam_buf),a
	ld	a,(4 * 28 + 3 + oam_buf)
	or	01h
	ld	(4 * 28 + 3 + oam_buf),a
	ret
  endif



