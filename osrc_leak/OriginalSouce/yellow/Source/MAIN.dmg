	db	usf_s,usf_o,usf_n,usf_y
	db	EOM
  endif

TimerCheck:
	ld	hl,usr_buf+2
	ld	e,(hl)
	inc	hl
	ld	d,(hl)

	inc	de
	ld	a,d
	cp	12
	jr	z,reset$

	ld	(hl),d
	dec	hl
	ld	(hl),e
	and	a
	ret
reset$:
	scf
	ret

