	extern	PikaVoicePlay
	ld	hl,PikaVoicePlay
	ld	b,BANK__PikaVoicePlay	;	ld	b,G_BANK3b
	call	bank_push_call
	ret

Mabataki:
	call	MabatakiCheck

	ld	a,(usr_buf+0)
	ld	e,a
	ld	d,00h
	ld	hl,SeqTable$
	add	hl,de
	add	hl,de
	ld	a,(hli)
	ld	h,(hl)
	ld	l,a
	jp	(hl)
SeqTable$:
	dw	Hold$
	dw	Close1$
	dw	wait$
	dw	wait$
	dw	Close2$
	dw	wait$
	dw	wait$
	dw	Close1$
	dw	wait$
	dw	wait$
	dw	Open$
	dw	Reset$
Reset$:
	xor	a
	ld	(usr_buf+0),a
Hold$:
	ret

Open$:
	ld	e,00000000b
	jr	Set$
Close1$:
	ld	e,00000100b
	jr	Set$
Close2$:
	ld	e,00001000b

Set$:
	ld	hl,oam_buf+2
	ld	c,8
loop$:
	ld	a,(hl)
	and	11110011b
	or	e
	ld	(hli),a
	inc	hl
	inc	hl
	inc	hl
	dec	c
	jr	nz,loop$
wait$:
	ld	hl,usr_buf+0
	inc	(hl)
	ret
MabatakiCheck:
	ld	hl,usr_buf+1
	ld	a,(hl)
	inc	(hl)
	and	a
	jr	z,On$
	cp	080h
	jr	z,On$
	cp	090h
	ret	nz
On$:
	ld	a,1
	ld	(usr_buf+0),a
	ret

