	call	BGMplay

start$:
	xor	a
	ld	(anime_buf),a
	ld	(usr_buf+0),a
	ld	(usr_buf+1),a
	ld	(usr_buf+2),a
	ld	(usr_buf+3),a
	ld	a,15
	ld	(usr_buf+4),a			;for Voice Test

start_loop$:
	call	TimerCheck
	jp	c,go_reset$

	call	wait_vb
	
	call	cont_repeat

	ld	a,(joy_status)
	cp	46h
	jr	z,next$
;	cp	06h
;	jr	z,Print$
;	cp	10h
;	jr	z,Voice$


;	ld	a,(joy_repeat)
;-----------------------------------------------; for debug & test
  ifn	pm_debug
	and	0dh		; A,Start,Select
  else
	and	09h		; A,Start
  endif
;-----------------------------------------------; for debug & test
	jr	nz,next$

	call	Mabataki
	jr	start_loop$

next$:
	ld	e,10
	call	VoiceStart

	call	pal_off_put_wait
	call	oam_clr
	xor	a
	ld	(window_y),a
	inc	a
	ld	(all_put_req),a
	call	dvram_cls

	ld	a,098h
	call	bgreq_change
	ld	a,09ch
	call	bgreq_change

	call	put_wait

	call	color_reset

	ld	a,(joy_status)

	ld	b,a

	and	046h
	cp	046h
	jp	z,Melting

;-----------------------------------------------; for debug & test
  ifn	pm_debug

	ld	a,b
	bit	2,a
	jp	z,play1
	
	ld	hl,option
	ld	b,03fh
	call	bank_push_call

	jp	(hl)

  else
	jp	play1
  endif

Print$:
	extern	PrintOut
	ld	hl,PrintOut
	ld	b,G_BANK3a
	call	bank_push_call
	jp	start$
