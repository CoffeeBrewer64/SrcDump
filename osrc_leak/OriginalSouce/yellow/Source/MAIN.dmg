 endif
	db	0e5h,0e6h,0e7h,0e8h,0e9h,0eah,0ebh,0ech,0edh ; GAMEFREAKinc.
	db	0ffh


get_out$:
	call	pop_vram		;with pokemon CG
	ld	c,36
	call	wait_vb_s

	ld	a,muslogoin
	call	SEplay

	ld	hl,TitleMapMakeLogo2
