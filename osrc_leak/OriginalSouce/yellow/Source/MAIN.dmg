	ld	(usr_buf+4),a
	ld	e,a
	ld	hl,PikaVoicePlay
	ld	b,BANK__PikaVoicePlay	;	ld	b,G_BANK3b
	call	bank_push_call
	xor	a
	ld	(usr_buf+2),a
	ld	(usr_buf+3),a
	jp	start_loop$

