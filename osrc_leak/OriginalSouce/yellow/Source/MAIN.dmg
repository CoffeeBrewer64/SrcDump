main:
	ld	hl,debug_hero
	ld	de,my_name
	call	name_copy

	ld	hl,debug_rival
	ld	de,rival_name
	call	name_copy

	xor	a
	ld	(window_y),a
	ld	(msg_speed),a
	ld	hl,game_mode + 0
	ld	(hli),a
	ld	(hli),a
	ld	(hl),a

	ld	a,MUSIC_BANK3_NO		; Title Music Bank
	ld	(music_bank_flg),a
	ld	(music_bank_flg_back),a

main_loop:
	call	pal_off
	ld	a,1				;by tama 960921 for save memory
	ld	(all_put_req),a
	xor	a
	ld	(wave_flg),a
	ld	(headerx),a			;headerx = 0 

	ld	a,040h
	ld	(headery),a

	ld	a,144
	ld	(window_y),a

	call	dvram_cls
	call	lcdc_stop			; LCD Control Stop Mode

	call	set_kana

	ld	hl,fnt_marushi	 		; Set maru C Font
	ld	de,8e00h			; Set "(C)1995-9"	
	ld	bc,005h*16
	ld	a,4
	call	chrset

 ifn  ASSEMBLE__ENGLISH
	ld	hl,fnt_marushi + 1c0h 		; Set '9' (1995-9'9')
	ld	de,8ee0h	
	ld	bc,001h*16
	ld	a,4
	call	chrset
 endif

	ld	hl,fnt_marushi + 130h 		; Set maru C Font
	ld	de,8e50h	
	ld	bc,009h*16
	ld	a,4
	call	chrset

	ld	hl,TitleCGXset
	ld	b,G_BANK3d				;29->3d 変更（村川）
	call	bank_push_call
	ld	hl,09800h
	ld	bc,0800h
	ld	a,spc@
	call	memset

	ld	hl,TitleMapMakeLogo1
	ld	b,G_BANK3d				;29->3d 変更（村川）
	call	bank_push_call

	call	PM64Init			;ポケモンスタジアム用ルーチン

	call	pika_under_maruc$

	call	push_vram_m
	call	pop_vram_m

	call	lcdc_on

	ld	hl,TitleMapMakePikatyu
	ld	b,G_BANK3d				;29->3d 変更（村川）
	call	bank_push_call
	
	ld	a,09bh
	call	bgreq_change
	call	push_vram

	ld	a,040h
	ld	(window_y),a

	call	pop_vram_m
	ld	a,098h
	call	bgreq_change

	ld	b,COL_OPNING
	call	color_set
	call	palset
	ld	a,11100000b
	ld	(OBP0),a
  ifn	pm_cgb
	call	change_cgb_obp0
  endif

	ld	bc,headery
	ld	hl,move_tbl$

logo_in_loop$:
	ld	a,(hli)
	and	a
	jr	z,get_out$

	ld	d,a

	cp	-3
	jr	nz,pass_se$

	extern	musgengerpunch
	ld	a,musgengerpunch
	call	SEplay

pass_se$:
	ld	a,(hli)
	ld	e,a
	call	up_loop$

	jr 	logo_in_loop$
	
move_tbl$:
	db	-4,010h,3,004h,-3,004h,2,002h,-2,002h,1,002h,-1,002h,0
up_loop$:
	call	wait_vb
	ld	a,(bc)
	add	a,d
	ld	(bc),a
	dec	e
	jr	nz,up_loop$
	ret


pika_under_maruc$:
	S_POS	2,17
	ld	de,marushi_msg$
marushi_loop$:
	ld	a,(de)
	inc	de
	cp	0ffh
	ret	z

	ld	(hli),a
	jr	marushi_loop$


marushi_msg$:
 ifn  ASSEMBLE__ENGLISH
;;;	db	0e0h,0e1h,0e2h,0e3h,0e4h,0eeh		; (C)1995-99
