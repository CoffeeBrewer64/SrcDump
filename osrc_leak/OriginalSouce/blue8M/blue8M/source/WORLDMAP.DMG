;================================================================;
;   MONS_TRADE_MAIN                                              ;
;        IN  : auto_move_val =  sellect_monster no.		 ;
;            : sel_item_no   =  get_monster no.			 ;
;            : mons_level    =  get_monster level		 ;
;        OUT : carry_flg   0 =  monter_trade OK 		 ;
;            :		   1 =  monter_trade NG 		 ;
;================================================================;
mons_trade_main:
	xor	a
	ld	(ef_size),a
	dec	a
	ld	(oam_flg),a
	call	cap_list
	push	af
	call	set_chr$
	pop	af
	ld	a,1
	jp	c,no_mons$

	ld	a,(auto_move_val + TRADE_MONS_No_@@)
	ld	b,a
	ld	a,(sel_item_no)
	cp	b
	ld	a,2
	jr	nz,no_mons$
	
	ld	a,(sel_item_pos)
	ld	hl,my_cap_data + 33
	ld	bc,CAPDATA_LEN
	call	mul_any

	ld	a,(hl)		;get_mons_level = my_mons_level
	ld	(mons_level),a

	ld	hl,comment_flag	;  set
	ld	a,(usr_buf)
	ld	c,a
	ld	b,1
	ld	a,B_BIT_CONTROL
	call	bank2bank

	ld	hl,koukan_msg
	call	put_win_msg

	ld	a,(sel_item_pos)
	push	af
	ld	a,(mons_level)
	push	af

	call	set_gauge
	call	set_koukandata
	ld	a,B_KOUKAN_DEMO_OYA
	call	bank2bank

	pop	af
	ld	(mons_level),a
	pop	af
	ld	(sel_item_pos),a

	ld	a,(auto_move_val + GET_MONS_No_@@)
	ld	(sel_item_no),a

	xor	a
	ld	(my_or_gein),a
	ld	(cap_or_mons),a
	call	sub_capsule
	ld	a,80h
	ld	(my_or_gein),a
	call	add_capsule_new

	call	set_koukanmons

;--------------------------- blue ordinal program ----------
	ld	hl,blue_special
	ld	b,05h
	call	bank_push_call
;--------------------------- blue ordinal program ----------
	call	dvram_cls

	call	set_chr$

	ld	b,3
	ld	hl,cellput_to_bg
	call	bank_push_call

;	call	set_koukanmons
	and	a			;C_flg clear mons_trade OK!
	ld	a,3
	jr	ret$

no_mons$:
	scf				;C_flg set   mons_trade NG!
ret$:
	ld	(auto_move_val + MSG_COUNT_@@),a
	ret

set_chr$:
	call	pal_off_put_wait

	call	set_objdata
	extern	map_rewrite2
	call	map_rewrite2

	call	pop_vram_m
;	ld	b,3
;	ld	hl,cellput_to_bg
;	call	bank_push_call

	call	put_wait

	call	color_reset
	ld	c,10
	call	wait_vb_s

	ld	b,3
	ld	hl,encount
	jp	bank_push_call


set_koukandata:
	ld	hl,usr_buf
	ld	a,(auto_move_val + TRADE_MONS_No_@@)
	ld	(hli),a				; usr_buf[0] 自分のポケモン番号
;	ld	a,(sel_item_no)
	ld	a,(auto_move_val + GET_MONS_No_@@)
	ld	(hl),a				; usr_buf[1] 相手のポケモン番号

	ld	hl,my_cap_oya
 ifn	ASSEMBLE__ENGLISH
	ld	bc,MONS_NAME_LEN
 else
	ld	bc,6
 endif
	ld	a,(sel_item_pos)
	call	mul_any

;	ld	hl,my_name			; 自分の名前
	ld	de,usr_buf + MY_OYA_NAME_@	; ld	de,usr_buf + 4	; usr_buf 4~9（５文字版）
 ifn	ASSEMBLE__ENGLISH
	ld	bc,MONS_NAME_LEN
 else
	ld	bc,6
 endif
	call	move$

	ld	hl,aite_name			; 相手の名前  ”トレーナー”
	ld	de,usr_buf + YOUR_OYA_NAME_@	; ld	de,usr_buf + 12	; usr_buf 12~17（５文字版）
	call	move$

	ld	de,gein_name 			; gein_name[] = aite_name[]  ”トレーナー”
	call	move$

	ld	hl,my_cap_data + 12
	ld	bc,CAPDATA_LEN
	ld	a,(sel_item_pos)
	call	mul_any				; return  HL= &自分のポケモン IDNo.

;	ld	hl,gb_no			; my_id_no
	ld	de,usr_buf + MY_MONS_IDNo_@	; ld	de,usr_buf + 10（５文字版）
	ld	bc,2
	call	move$

	extern	rnd
	call	rnd
	ld	hl,random			; 相手のポケモン IDNo. (乱数で決定）aite_id_no
	ld	de,usr_buf + YOUR_MONS_IDNo_@	; ld	de,usr_buf + 18
	jp	block_move

move$:
	push	hl
	push	bc
	call	block_move
	pop	bc
	pop	hl
	ret

set_koukanmons:
 ifn	ASSEMBLE__ENGLISH
	ld	hl,my_cap_name
	ld	bc, MONS_NAME_LEN
	call	set_ram$			; return  DE = HL + BC*((my_cap_tbl)-1)	
	ld	hl,auto_move_val + PET_NAME_@@
	ld	bc, MONS_NAME_LEN		;block_move(&my_cap_name[(my_cap_tbl)-1],
	call	block_move			;	&auto_move_val[PET_NAME_@@], MONS_NAME_LEN)

	ld	hl,my_cap_oya
	ld	bc, MONS_NAME_LEN
	call	set_ram$
	ld	hl,aite_name
	ld	bc, MONS_NAME_LEN		;block_move(&my_cap_oya[(my_cap_tbl)-1],
	call	block_move			;		&aite_name[0], MONS_NAME_LEN)

	ld	hl,my_cap_data + 12		; my_cap_data[][12] == ポケモン IDNo.
	ld	bc,CAPDATA_LEN
	call	set_ram$
	ld	hl,usr_buf + YOUR_MONS_IDNo_@	; ld	hl,usr_buf + 18
	ld	bc,2				;block_move(&my_cap_data[(my_cap_tbl)-1][12],
	jp	block_move			;		&usr_buf[YOUR_MONS_IDNo_@], 2)
 else
	ld	hl,my_cap_name
	ld	bc,6
	call	set_ram$
	ld	hl,auto_move_val + PET_NAME_@@
	ld	bc,6
	call	block_move

	ld	hl,my_cap_oya
	ld	bc,6
	call	set_ram$
	ld	hl,aite_name
	ld	bc,6
	call	block_move

	ld	hl,my_cap_data + 12	
	ld	bc,CAPDATA_LEN
	call	set_ram$
	ld	hl,usr_buf + 18
	ld	bc,2
	jp	block_move
 endif
 

set_ram$:
	ld	a,(my_cap_tbl)
	dec	a
	call	mul_any
	ld	e,l
	ld	d,h			; return  DE = HL + BC*((my_cap_tbl)-1)
	ret
	

aite_name:
	db	trainer@,EOM
 ifn	ASSEMBLE__ENGLISH
	db	EOM,EOM,EOM,EOM,EOM,EOM,EOM,EOM,EOM	; MONS_NAME_LEN (== 11) 転送されるので
 endif							; trade_msg_tbl:以降のゴミデータ
							; 混入を防ぐ為、残りを EOM にしておく
; 注意！）$FE データが混入すると通信交換時の転送で引っかかり "play2.dmg" block_move_loop2$: を参照
; 交換ウィンドウの相手ニックネームの表示等がおかしくなる。


