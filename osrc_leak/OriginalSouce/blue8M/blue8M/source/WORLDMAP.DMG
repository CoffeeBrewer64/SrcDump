;================================================================;
;   MONSTER_TRADE                                                ;
;        IN  : usr_buf  =  sellect_people_pos 			 ;
;================================================================;
	public	monster_trade
monster_trade:
	call	push_vram_m
	ld	hl,mons_trade_tbl
	ld	a,(usr_buf)
;--------------------------- blue ordinal program ----------
 ifn  pm_jmsg
;	swap	a
;	srl	a
	ld	b,a
	swap	a
	srl	a
	sub	b		; A *= 7  mons_trade_tbl[7*(usr_buf)] の１組のデータ長は７バイト
 else
	ld	b,a
	swap	a
	sub	b
	sub	b		; A *= 14  mons_trade_tbl[14*(usr_buf)] 英語版１組のデータ長は１４
 endif
;--------------------------- blue ordinal program ----------
	ld	c,a
	ld	b,0
	add	hl,bc		; HL= mons_trade_tbl[7*(usr_buf)]	; 英語版は [14*(usr_buf)]

	ld	a,(hli)					;trade_mons_no.
	ld	(auto_move_val + TRADE_MONS_No_@@),a
	ld	a,(hli)					;get_mons_no.
	ld	(auto_move_val + GET_MONS_No_@@),a
;	ld	(sel_item_no),a
	ld	a,(hli)		;msg_kind_no.

	push	af
	ld	de,auto_move_val + PET_NAME_@@		;save pet_name

;--------------------------- blue ordinal program ----------
 ifn  pm_jmsg
;	ld	bc,MONS_NAME_LEN
;	call	block_move
;	ld	a,EOM
;	ld	(auto_move_val + 21),a
	ld	bc,4			; 注意！ mons_trade_tbl[] の文字長は全て４文字
	call	block_move
	ld	a,EOM
	ld	(de),a
 else
	ld	bc,MONS_NAME_LEN
	call	block_move
 endif
;--------------------------- blue ordinal program ----------

	pop	af
	ld	l,a
	ld	h,0
	ld	de,trade_msg_tbl
	add	hl,hl
	add	hl,de

	ld	a,(hli)
	ld	(auto_move_val + MSG_ADRS_@@),a		;save msg_address_tbl
	ld	a,(hl)
	ld	(auto_move_val + MSG_ADRS_@@ +1),a

	ld	a,(auto_move_val + TRADE_MONS_No_@@)	;leave_mons_name in str_buf
	ld	de,auto_move_val + TRADE_MONS_NAME_@@
	call	monsname_save

	ld	a,(auto_move_val + GET_MONS_No_@@)
;	ld	a,(sel_item_no)				;get_mons_name in table_data
	ld	de,auto_move_val + GET_MONS_NAME_@@
	call	monsname_save

	ld	hl,comment_flag	;  check
	ld	a,(usr_buf)
	ld	c,a
	ld	b,2
	ld	a,B_BIT_CONTROL
	call	bank2bank

	ld	a,c
	and	a
	ld	a,4
	ld	(auto_move_val + MSG_COUNT_@@),a	;msg_count init
	jr	nz,put_trade_msg

	xor	a
	ld	(auto_move_val + MSG_COUNT_@@),a	;msg_count init
	call	put_trade_msg
	ld	a,1
	ld	(auto_move_val + MSG_COUNT_@@),a

	call	yes_no
	ld	a,(allow_cnt)
	and	a
	jr	nz,put_trade_msg

	call	mons_trade_main
	jr	c,put_trade_msg
	
	ld	hl,get_msg
	call	put_win_msg

put_trade_msg:
	ld	hl,auto_move_val + MSG_COUNT_@@
	ld	a,(hld)
	ld	e,a
	ld	d,0
	ld	a,(hld)
	ld	l,(hl)
	ld	h,a
	add	hl,de
	add	hl,de

	ld	a,(hli)
	ld	h,(hl)
	ld	l,a
	jp	put_win_msg


monsname_save:
	push	de
	ld	(in_dat),a
	call	get_mons_name
	ld	hl,table_data
	pop	de
	ld	bc,MONS_NAME_LEN
	jp	block_move

	
